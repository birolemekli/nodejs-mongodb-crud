<div class="container" style=padding-top:50px;>
    <table class="table table-hover  table-striped">
        <thead>
            <tr>
                <th>#</th>
                <th>Ad</th>
                <th>Soyad</th>
                <th>E Mail</th>
                <th>Kullanici Ad</th>
                <th>İşlemler</th>
            </tr>

        </thead>
        <tbody>
        <%for(var i=0;i<kullanicilar.length;i++){%>
        
                <tr>
                    <th><%=i+1%></th>
                    <th><%=kullanicilar[i].ad%></th>
                    <th><%=kullanicilar[i].soyad%></th>
                    <th><%=kullanicilar[i].email%> </th>
                    <th><%=kullanicilar[i].kullaniciAdi%></th>
                    <th><a href="/login/kullaniciSil/<%=kullanicilar[i].kullaniciAdi%>">sil</a></th>
                    <th><a href="/login/kullaniciGuncelle/<%=kullanicilar[i]._id%>">güncelle</a></th>
                </tr>
        <%}%>
                
        </tbody>

    </table>

</div>